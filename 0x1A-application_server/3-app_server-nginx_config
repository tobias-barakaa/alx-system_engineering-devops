server {
    listen 80;
    server_name _;

    location /airbnb-dynamic/number_odd_or_even/ {
        proxy_pass http://127.0.0.1:5001; # Forward requests to Gunicorn on port 5001
    }

    location / {
        proxy_pass http://127.0.0.1:5000; # Forward other requests to Gunicorn on port 5000
    }
}
